<div>
  <h2>Perfil de usuario</h2>

  <h2>{{oneUser.username}}</h2>
  <img src="{{oneUser.profileImg}}" alt="" />
  <p>{{oneUser.email}}</p>
  <p>{{oneUser.dateborn}}</p>
  <div>

    <h3>Lugares Favoritos</h3>
    {{#each oneUser.placeFav}}
      <li>{{this.name}}</li>
      <br>
      <form action="/user/{{this._id}}/delete" method="post"><button>Eliminar Fav</button></form>
      <br>
      <a href="/places/{{this._id}}/details"><button>Detalles</button></a>
      <br>
    {{/each}}
  </div> 

</div>